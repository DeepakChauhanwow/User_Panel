{
    "index": "/index.html",
    "assetGroups": [
      {
        "name": "app",
        "installMode": "prefetch",
        "resources": {
          "files": [
            "/favicon.ico",
            "/index.html",
            "/*.bundle.css",
            "/*.bundle.js",
            "*.js",
            "/*.chunk.js"          ]
        },
        "cacheConfig": {
          "maxSize": 100,
          "maxAge": "86400",
          "strategy": "performance"
        }
      },
      {
        "name": "assets",
        "installMode": "lazy",
        "updateMode": "prefetch",
        "resources": {
          "files": [
            "/assets/**"
          ]
        }
      }
    ]
  }
  